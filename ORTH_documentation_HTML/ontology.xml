<?xml version="1.0"?>
<rdf:RDF xmlns="http://purl.org/net/orth#"
     xml:base="http://purl.org/net/orth"
     xmlns:orth="http://purl.org/net/orth#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://purl.org/net/orth">
        <owl:versionInfo xml:lang="en">This ontology is the second version of the orthology ontology first published in 2015 and available at http://purl.bioontology.org/ontology/ORTH .
@authors T.M. de Farias, H. Chiba, J.T. Fernandez-Breis, E. Antezana, D. Kuznetsov, C. Dessimoz and F. Villiers.</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/net/orth#inDatabase -->


    <owl:AnnotationProperty rdf:about="http://purl.org/net/orth#inDatabase">
        <rdfs:comment xml:lang="en">A property to define the database an orthology dataset belongs to.</rdfs:comment>
        <rdfs:label xml:lang="en">in database</rdfs:label>
        <rdfs:domain rdf:resource="http://purl.org/net/orth#OrthologyDataset"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#altLabel -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#altLabel">
        <rdfs:comment xml:lang="en">The range of skos:altLabel is the class of RDF plain literals.</rdfs:comment>
        <rdfs:comment xml:lang="en">skos:prefLabel, skos:altLabel and skos:hiddenLabel are pairwise disjoint properties.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <rdfs:label xml:lang="en">alternative label</rdfs:label>
        <skos:definition xml:lang="en">An alternative lexical label for a resource.</skos:definition>
        <skos:example xml:lang="en">Acronyms, abbreviations, spelling variants, and irregular plural/singular forms may be included among the alternative labels for a concept. Mis-spelled terms are normally included as hidden labels (see skos:hiddenLabel).</skos:example>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#definition -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#definition">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <rdfs:label xml:lang="en">definition</rdfs:label>
        <skos:definition xml:lang="en">A statement or formal explanation of the meaning of a concept.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#note"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#example -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#example">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <rdfs:label xml:lang="en">example</rdfs:label>
        <skos:definition xml:lang="en">An example of the use of a concept.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#note"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#note -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#note">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <rdfs:label xml:lang="en">note</rdfs:label>
        <skos:definition xml:lang="en">A general note, for any purpose.</skos:definition>
        <skos:scopeNote xml:lang="en">This property may be used directly, or as a super-property for more specific note types.</skos:scopeNote>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#prefLabel -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#prefLabel">
        <rdfs:comment xml:lang="en">A resource has no more than one value of skos:prefLabel per language tag, and no more than one value of skos:prefLabel without language tag.</rdfs:comment>
        <rdfs:comment xml:lang="en">The range of skos:prefLabel is the class of RDF plain literals.</rdfs:comment>
        <rdfs:comment xml:lang="en">skos:prefLabel, skos:altLabel and skos:hiddenLabel are pairwise
      disjoint properties.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <rdfs:label xml:lang="en">preferred label</rdfs:label>
        <skos:definition xml:lang="en">The preferred lexical label for a resource, in a given language.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#scopeNote -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#scopeNote">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <rdfs:label xml:lang="en">scope note</rdfs:label>
        <skos:definition xml:lang="en">A note that helps to clarify the meaning and/or the use of a concept.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#note"/>
    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.obolibrary.org/obo/CDAO_0000178 -->


    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/CDAO_0000178">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/CDAO_0000194 -->


    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/CDAO_0000194">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">part_of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/RO_0001018 -->


    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/RO_0001018">
        <rdfs:label xml:lang="en">contained in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/RO_0002162 -->


    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/RO_0002162">
        <rdfs:comment xml:lang="en">Connects a biological entity to its taxon of origin.</rdfs:comment>
        <rdfs:label xml:lang="en">in taxon</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/RO_0002350 -->


    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/RO_0002350">
        <rdfs:label xml:lang="en">member of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#hasAncestralEvolutionaryEvent -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#hasAncestralEvolutionaryEvent">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/orth#hasEvolutionaryEvent"/>
        <rdfs:label xml:lang="en">has ancestral evolutionary event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#hasCluster -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#hasCluster">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/CDAO_0000178"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/orth#inDataset"/>
        <rdfs:domain rdf:resource="http://purl.org/net/orth#OrthologyDataset"/>
        <rdfs:range rdf:resource="http://purl.org/net/orth#HomologsCluster"/>
        <rdfs:comment xml:lang="en">Inverse property of inDataset.  It permits to navigate to the clusters included in a particular dataset.</rdfs:comment>
        <rdfs:label xml:lang="en">has cluster</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#hasCoOrthologousMember -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#hasCoOrthologousMember">
        <rdfs:subPropertyOf rdf:resource="http://semanticscience.org/resource/SIO_000028"/>
        <rdfs:domain rdf:resource="http://purl.org/net/orth#CoOrthologyRelation"/>
        <rdfs:range rdf:resource="http://purl.org/net/orth#SequenceUnit"/>
        <rdfs:label xml:lang="en">has co-orthologous member</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#hasDescendantEvolutionaryEvent -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#hasDescendantEvolutionaryEvent">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/orth#hasEvolutionaryEvent"/>
        <rdfs:label xml:lang="en">has descendant evolutionary event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#hasEvolutionaryEvent -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#hasEvolutionaryEvent">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/orth#HomologsCluster"/>
                    <rdf:Description rdf:about="http://purl.org/net/orth#OrthologyRelation"/>
                    <rdf:Description rdf:about="http://purl.org/net/orth#ParalogyRelation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/CDAO_0000064"/>
        <rdfs:label xml:lang="en">has evolutionary event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#hasHomolog -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#hasHomolog">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/CDAO_0000178"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/net/orth#SequenceUnit"/>
        <rdfs:range rdf:resource="http://purl.org/net/orth#SequenceUnit"/>
        <rdfs:label xml:lang="en">has homolog</rdfs:label>
        <skos:altLabel xml:lang="en">is homologous to</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#hasHomologousMember -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#hasHomologousMember">
        <rdfs:subPropertyOf rdf:resource="http://semanticscience.org/resource/SIO_000028"/>
        <owl:inverseOf rdf:resource="http://purl.org/net/orth#isHomologousMember"/>
        <rdfs:domain rdf:resource="http://purl.org/net/orth#HomologsCluster"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/orth#HomologsCluster"/>
                    <rdf:Description rdf:about="http://purl.org/net/orth#SequenceUnit"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Property that permits to define the membership of a cluster of homologs or a sequence unit to a clusters of homologs.</rdfs:comment>
        <rdfs:label xml:lang="en">has homologous member</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#hasInparalogousMember -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#hasInparalogousMember">
        <rdfs:subPropertyOf rdf:resource="http://semanticscience.org/resource/SIO_000028"/>
        <rdfs:domain rdf:resource="http://purl.org/net/orth#CoOrthologyRelation"/>
        <rdfs:range rdf:resource="http://purl.org/net/orth#InparalogyRelation"/>
        <rdfs:label xml:lang="en">has inparalogous member</rdfs:label>
        <skos:altLabel xml:lang="en">has in-paralogous member</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#hasLeastDivergedOrtholog -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#hasLeastDivergedOrtholog">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/orth#hasOrtholog"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/net/orth#SequenceUnit"/>
        <rdfs:range rdf:resource="http://purl.org/net/orth#SequenceUnit"/>
        <rdfs:comment xml:lang="en">Least diverged orthologs (LDOs) are the genes in two different organisms that have diverged the least since their common ancestor and are most likely to retain the greatest functional similarities.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://pantherdb.org/genes/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">has least diverged ortholog</rdfs:label>
        <skos:altLabel xml:lang="en">is least divergent ortholog to</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#hasOrtholog -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#hasOrtholog">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/orth#hasHomolog"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:comment xml:lang="en">Orthologous pairwise relation between genes/proteins. For example, A Gene(A) hasOrtholog Gene(B). This property can be materialized or inferred by applying a logical rule.</rdfs:comment>
        <rdfs:label xml:lang="en">has ortholog</rdfs:label>
        <rdfs:label xml:lang="en">is ortholog to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#hasParalog -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#hasParalog">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/orth#hasHomolog"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:comment xml:lang="en">Paralogous pairwise relation between genes/proteins. For example, a Gene(A) hasParalog Gene(B). This property can be materialized or inferred by applying a logical rule.</rdfs:comment>
        <rdfs:label xml:lang="en">has paralog</rdfs:label>
        <rdfs:label xml:lang="en">is paralog to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#hasTaxonomicRange -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#hasTaxonomicRange">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/CDAO_0000178"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/orth#HomologsCluster"/>
                    <rdf:Description rdf:about="http://purl.org/net/orth#OrthologyRelation"/>
                    <rdf:Description rdf:about="http://purl.org/net/orth#ParalogyRelation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/net/orth#TaxonomicRange"/>
        <rdfs:comment xml:lang="en">Property that associates a cluster of homologs with the taxonomic range at which it has been calculated</rdfs:comment>
        <rdfs:label xml:lang="en">has taxonomic level</rdfs:label>
        <skos:altLabel xml:lang="en">has taxonomic range</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#hasXenolog -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#hasXenolog">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/orth#hasHomolog"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:comment xml:lang="en">Xenologous pairwise relation between genes/proteins. For example, a Gene(A) hasXenolog Gene(B).</rdfs:comment>
        <rdfs:label xml:lang="en">has xenolog</rdfs:label>
        <skos:altLabel xml:lang="en">is xenolog to</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#inDataset -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#inDataset">
        <rdfs:domain rdf:resource="http://purl.org/net/orth#GeneTreeNode"/>
        <rdfs:range rdf:resource="http://purl.org/net/orth#OrthologyDataset"/>
        <rdfs:comment xml:lang="en">Property that provides the dataset associated with a gene tree node</rdfs:comment>
        <rdfs:label xml:lang="en">in dataset</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#isHomologousMember -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#isHomologousMember">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/RO_0002350"/>
        <rdfs:comment xml:lang="en">Inverse of has homologous member. This property permits to find the cluster in which a cluster or sequence unit is included.</rdfs:comment>
        <rdfs:label xml:lang="en">is homologous member</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/orth#organism -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/orth#organism">
        <rdfs:domain rdf:resource="http://purl.org/net/orth#SequenceUnit"/>
        <rdfs:range rdf:resource="http://purl.org/net/orth#Organism"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.uniprot.org/core/replaces -->


    <owl:ObjectProperty rdf:about="http://purl.uniprot.org/core/replaces">
        <rdfs:domain rdf:resource="http://purl.uniprot.org/core/Taxon"/>
        <rdfs:range rdf:resource="http://purl.uniprot.org/core/Taxon"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A resource that is replaced by this resource.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/SIO_000028 -->


    <owl:ObjectProperty rdf:about="http://semanticscience.org/resource/SIO_000028">
        <rdfs:label xml:lang="en">has part</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://semanticscience.org/resource/SIO_010078 -->


    <owl:ObjectProperty rdf:about="http://semanticscience.org/resource/SIO_010078">
        <rdfs:label xml:lang="en">encodes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms/identifier -->


    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/identifier">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">identifier</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/net/orth#isLeastDivergedOrtholog -->


    <owl:DatatypeProperty rdf:about="http://purl.org/net/orth#isLeastDivergedOrtholog">
        <rdfs:domain rdf:resource="http://purl.org/net/orth#OrthologyRelation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:label xml:lang="en">is least diverged ortholog</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/net/orth#taxRange -->


    <owl:DatatypeProperty rdf:about="http://purl.org/net/orth#taxRange">
        <rdfs:domain rdf:resource="http://purl.org/net/orth#TaxonomicRange"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Property that stores the name of the taxonomic level at which a given cluster of homologs has been calculated</rdfs:comment>
        <rdfs:label xml:lang="en">taxonomic level</rdfs:label>
        <skos:altLabel xml:lang="en">tax range</skos:altLabel>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/net/orth#taxRangeId -->


    <owl:DatatypeProperty rdf:about="http://purl.org/net/orth#taxRangeId">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="en">This property links a cluster of homologs with its taxonomic level.</rdfs:comment>
        <rdfs:label xml:lang="en">taxonomic level identifier</rdfs:label>
        <skos:altLabel xml:lang="en">tax range id</skos:altLabel>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/pav/version -->


    <owl:DatatypeProperty rdf:about="http://purl.org/pav/version">
        <rdfs:label xml:lang="en">Version</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.uniprot.org/core/mnemonic -->


    <owl:DatatypeProperty rdf:about="http://purl.uniprot.org/core/mnemonic">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">A easy to remember identifier for a UniProtKB entry, but it is not a stable identifier and should not be used by programs to identify entries.</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A rememberable string that can be used to find entries, not a stable identifier!</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mnemonic</rdfs:label>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.uniprot.org/manual/entry_name</rdfs:seeAlso>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.obolibrary.org/obo/CDAO_0000012 -->


    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CDAO_0000012">
        <rdfs:comment xml:lang="en">Tree with a root node, which is the common ancestor to all the nodes in the tree</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">RootedTree</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CDAO_0000064 -->


    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CDAO_0000064">
        <rdfs:comment xml:lang="en">Evolutionary event</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">cladogeneticChange</rdfs:label>
        <skos:altLabel xml:lang="en">clado genetic change</skos:altLabel>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CDAO_0000077 -->


    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CDAO_0000077">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CDAO_0000064"/>
        <rdfs:comment xml:lang="en">Evolutionary event that consists on the duplication of a gene</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">geneDuplication</rdfs:label>
        <skos:altLabel xml:lang="en">gene duplication</skos:altLabel>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CDAO_0000121 -->


    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CDAO_0000121">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CDAO_0000064"/>
        <rdfs:comment xml:lang="en">Evolutionary event related with the evolution of a gene in different species</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">speciation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CDAO_0000140 -->


    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CDAO_0000140">
        <rdfs:comment xml:lang="en">Basic unit of graphs and trees</rdfs:comment>
